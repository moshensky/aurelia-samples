template
  modal(showing.bind="confirmDialogShowing", options.bind="{ backdrop: 'static' }")
    modal-header(title="Name Goes Here", close.call="onConfirmDialogCancelClick()")
    modal-body(content-view.bind="view-model-name-goes-here", content-model.bind="model-name-goes-here")
    modal-footer
      button.btn(click.trigger="onConfirmDialogOkClick()") Ok
      au-button(text="Cancel", click.call="onConfirmDialogCancelClick()")
